<h1>Tasklist</h1>
<h2>Log in</h2>

<%= form_with(url: login_path, scope: :session, local: true) do |f| %>

<%= f.label :email, 'Email' %>
<%= f.email_field :email %>

<%= f.label :password, 'Password' %>
<%= f.password_field :password %>

<%= f.submit 'Log in' %>

<% end %>

<p>New User? <%= link_to 'Sign up now!', signup_path %></p>

